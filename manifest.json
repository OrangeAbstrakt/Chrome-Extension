{
    "manifest_version" : 2,
    "name" : "SuperCuts",
    "description" : "Launch your everyday resources from one place",
    "version" : "1.0.0",
    "icons" : {"128": "icon_128.png"},
    "browser_action" : {
        "default_icon" : "icon.png",
        "default_popup" : "popup.html"
    },
    "options_page": "options.html",
    "permissions" : [
        "activeTab",
        "tabs"
]
}